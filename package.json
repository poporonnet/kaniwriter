{
  "name": "kaniwriter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:ceres": "tsc && NODE_OPTIONS=--max-old-space-size=4096 vite build --mode ceres",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "format": "eslint src --fix && prettier --write \"src/**/*.{js,jsx,ts,tsx,json,md}\"",
    "prettier": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,md}\"",
    "check": "prettier --check \"src/**/*.{ts,tsx}\" && tsc -p . --noEmit && eslint src --ext ts,tsx --max-warnings 0"
  },
  "packageManager": "pnpm@9.12.3",
  "dependencies": {
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.15",
    "@mui/joy": "5.0.0-beta.48",
    "@mui/material": "^5.13.2",
    "anser": "^2.1.1",
    "base64-js": "^1.5.1",
    "i18next": "^23.11.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^15.0.0",
    "react-router-dom": "^6.11.2"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/node": "^20.8.10",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/w3c-web-serial": "^1.0.6",
    "@vitejs/plugin-react": "^4.0.0",
    "path": "^0.12.7",
    "shiki": "^1.7.0",
    "typescript": "^5.0.2",
    "vite": "^5.0.0"
  }
}
